{
  "name": "agentry",
  "author": "Colin D'Souza",
  "license": "MIT",
  "version": "0.0.1",
  "description": "React reconciler-based framework for declarative AI agent orchestration",
  "repository": {
    "type": "git",
    "url": "https://github.com/colinds/agentry"
  },
  "homepage": "https://github.com/colinds/agentry",
  "bugs": {
    "url": "https://github.com/colinds/agentry/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "react",
    "declarative",
    "reconciler",
    "anthropic",
    "claude",
    "framework"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build.ts && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepack": "cp ../../README.md . && cp ../../LICENSE.md .",
    "postpack": "rm README.md LICENSE.md"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.1",
    "eventemitter3": "^5.0.1",
    "react-reconciler": "^0.33.0",
    "scheduler": "^0.27.0",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-reconciler": "^0.32.3",
    "@types/scheduler": "^0.26.0"
  }
}
